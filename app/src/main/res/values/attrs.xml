<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HorizontalProgressBarWithNumber">
        <attr name="progress_unreached_color_a" format="color"/>
        <attr name="progress_reached_color_a" format="color"/>
        <attr name="progress_reached_bar_height_a" format="dimension"/>
        <attr name="progress_unreached_bar_height_a" format="dimension"/>
        <attr name="progress_text_size_a" format="dimension"/>
        <attr name="progress_text_color_a" format="color"/>
        <attr name="progress_text_offset_a" format="dimension"/>
        <attr name="progress_text_visibility_a" format="enum">
            <enum name="visible" value="0"/>
            <enum name="invisible" value="1"/>
        </attr>
    </declare-styleable>



</resources>